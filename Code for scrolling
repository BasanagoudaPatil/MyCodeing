public static void scrolltoset() {
      Dimension size = driver.manage().window().getSize();
      int starty = (int) (size.getHeight() * 0.70);
      int endy = (int) (size.getHeight() * 0.30);
      int XCoordinates = size.getWidth();
      driver.swipe(XCoordinates/2,starty,XCoordinates/2,endy, 1000);

		  
	}	
